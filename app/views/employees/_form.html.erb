<div>
<%= f.label :name, placeholder: "Enter your name" %>
  <%= f.text_field :name, class: "form-control" %>
</div>
<div>
  <%= f.label :email, placeholder: "Enter your email" %>
  <%= f.text_field :email, class: "form-control" %>
</div>
<div>
  <%= f.label :phone_number, placeholder: "Enter your phone number" %>
  <%= f.text_field :phone_number, class: "form-control" %>
</div>
<div>
  <%= f.submit "Submit", class: "btn btn-primary"  %>
</div>